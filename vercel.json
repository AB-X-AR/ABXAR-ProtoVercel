{
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "headers": {
          "Authorization": "Basic ${process.env.VERCEL_USERNAME}:${process.env.VERCEL_PASSWORD}"
        },
        "status": 403
      }
    ]
  }
  